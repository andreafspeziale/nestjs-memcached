{
  "name": "@andreafspeziale/nestjs-memcached",
  "version": "2.0.2",
  "description": "A Memcached module and service for Nest - modern, fast, powerful node.js web framework",
  "author": "Andrea Francesco Speziale",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/andreafspeziale/nestjs-memcached"
  },
  "bugs": {
    "url": "https://github.com/andreafspeziale/nestjs-memcached/issues"
  },
  "homepage": "https://github.com/andreafspeziale/nestjs-memcached#readme",
  "scripts": {
    "prepare": "husky install",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"lib/**/*.ts\"",
    "lint": "eslint 'lib/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "release": "release-it"
  },
  "keywords": [
    "nest",
    "nestjs",
    "cache",
    "memcached",
    "mcrouter"
  ],
  "peerDependencies": {
    "@nestjs/common": "^9.1.2",
    "memcached": "^2.2.2"
  },
  "dependencies": {
    "@nestjs/common": "^9.1.2",
    "bluebird": "3.7.2",
    "memcached": "^2.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@nestjs/config": "2.2.0",
    "@nestjs/platform-express": "9.1.2",
    "@nestjs/testing": "9.1.2",
    "@release-it/conventional-changelog": "5.1.0",
    "@types/bluebird": "3.5.37",
    "@types/jest": "29.0.3",
    "@types/memcached": "2.2.7",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "eslint": "8.23.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.1",
    "jest": "29.0.3",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "release-it": "15.4.3",
    "rimraf": "3.0.2",
    "ts-jest": "29.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.ts": "npm run lint"
  }
}
